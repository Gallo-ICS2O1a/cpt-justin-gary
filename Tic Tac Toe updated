board = [
         [3],
         [3]
        ]
cellWidth, cellHeight = int, int
player = 1


def setup():
    size(600,400)
    rectMode(CORNER)
    ellipseMode(CORNER)
    
    cellWidth = width / 3
    cellHeight = height / 3

    clearBoard()
    
def draw():
    background(255)
    drawBoard()
    if boardfilled():
        drawGameOver()
        
def drawGameOver():
    fill(255)
    textSize(width / 7)
    textAlign(CENTER,CENTER)
    text("Game Over!", width / 2, height / 2)
    
def mousePressed():
    if boardfilled():
        clearBoard()
    else:
        playerMove()
